<resources>

	<style name="Theme.Drillingjobs" parent="@android:style/Theme.Holo.Light">
		<item name="android:actionBarItemBackground">@drawable/selectable_background_drillingjobs</item> 
		<item name="android:popupMenuStyle">@style/PopupMenu.Drillingjobs</item> 
		<item name="android:dropDownListViewStyle">@style/DropDownListView.Drillingjobs</item> 
		<item name="android:actionBarTabStyle">@style/ActionBarTabStyle.Drillingjobs</item> 
		<item name="android:actionDropDownStyle">@style/DropDownNav.Drillingjobs</item> 
		<item name="android:actionBarStyle">@style/ActionBar.Solid.Drillingjobs</item> 
		<item name="android:actionModeBackground">@drawable/cab_background_top_drillingjobs</item> 
		<item name="android:actionModeSplitBackground">@drawable/cab_background_bottom_drillingjobs</item> 
		<item name="android:actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Drillingjobs</item>
  	</style>
  	
  	<style name="ActionBar.Solid.Drillingjobs" parent="@android:style/Widget.Holo.Light.ActionBar.Solid">
		<item name="android:background">@drawable/ab_solid_drillingjobs</item> 
		<item name="android:backgroundStacked">@drawable/ab_stacked_solid_drillingjobs</item> 
		<item name="android:backgroundSplit">@drawable/ab_bottom_solid_drillingjobs</item> 
		<item name="android:progressBarStyle">@style/ProgressBar.Drillingjobs</item> 
		<item name="android:titleTextStyle">@style/MyTheme.ActionBar.TitleTextStyle</item>
  	</style>
  	
  	<style name="MyTheme.ActionBar.TitleTextStyle" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
    	<item name="android:textColor">@android:color/white</item>
	</style>
  	
	<style name="ActionBar.Transparent.Drillingjobs" parent="@android:style/Widget.Holo.Light.ActionBar">
		<item name="android:background">@drawable/ab_transparent_drillingjobs</item> 
		<item name="android:progressBarStyle">@style/ProgressBar.Drillingjobs</item> 
	</style>
	
	<style name="PopupMenu.Drillingjobs" parent="@android:style/Widget.Holo.Light.ListPopupWindow">
		<item name="android:popupBackground">@drawable/menu_dropdown_panel_drillingjobs</item> 
	</style>
	
	<style name="DropDownListView.Drillingjobs" parent="@android:style/Widget.Holo.Light.ListView.DropDown">
		<item name="android:listSelector">@drawable/selectable_background_drillingjobs</item> 
	</style>
	
	<style name="ActionBarTabStyle.Drillingjobs" parent="@android:style/Widget.Holo.Light.ActionBar.TabView">
		<item name="android:background">@drawable/tab_indicator_ab_drillingjobs</item> 
	</style>
	
	<style name="DropDownNav.Drillingjobs" parent="@android:style/Widget.Holo.Light.Spinner">
		<item name="android:background">@drawable/spinner_background_ab_drillingjobs</item> 
		<item name="android:popupBackground">@drawable/menu_dropdown_panel_drillingjobs</item> 
		<item name="android:dropDownSelector">@drawable/selectable_background_drillingjobs</item> 
	</style>
	
	<style name="ProgressBar.Drillingjobs" parent="@android:style/Widget.Holo.Light.ProgressBar.Horizontal">
		<item name="android:progressDrawable">@drawable/progress_horizontal_drillingjobs</item> 
	</style>
	
	<style name="ActionButton.CloseMode.Drillingjobs" parent="@android:style/Widget.Holo.Light.ActionButton.CloseMode">
		<item name="android:background">@drawable/btn_cab_done_drillingjobs</item> 
	</style>
	
	<!--  this style is only referenced in a Light.DarkActionBar based theme --> 
	<style name="Theme.Drillingjobs.Widget" parent="@android:style/Theme.Holo">
		<item name="android:popupMenuStyle">@style/PopupMenu.Drillingjobs</item> 
		<item name="android:dropDownListViewStyle">@style/DropDownListView.Drillingjobs</item> 
	</style>
	
	<style name="CustomDialog" parent="@android:style/Theme.Dialog">
		<item name="android:windowNoTitle">true</item>
    </style>

</resources>
